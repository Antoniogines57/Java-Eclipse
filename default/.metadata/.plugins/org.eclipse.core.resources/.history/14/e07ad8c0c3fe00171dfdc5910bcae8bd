package seccion28_class91;

public class Producto {
	private int ID;
	private String nombre;
	private double precio;
	private static int contProd;
	
	public Producto() {
		ID=contProd;
		contProd++;
	}
	public Producto(String nombre, double precio) {
		this();
		this.nombre=new String(nombre);
		this.precio=precio;
	}
}
