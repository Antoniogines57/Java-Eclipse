package cortana;

public class Fecha {
	private int dia;
	private int mes;
	private int year;
	
	public Fecha(int dia, int mes, int anyo) {
		if(dia<1||dia>31||mes<1||mes>12||year<2012) {
			System.out.println("Fecha introducida no valida.");
		}
		else {
			this.dia=dia; this.mes=mes; this.year=anyo;
		}
	public int getDia() {
		return dia;
	}
	
	public int getMes() {
		return mes;
	}
	public int getYear() {
		return year;
	}
	
}
