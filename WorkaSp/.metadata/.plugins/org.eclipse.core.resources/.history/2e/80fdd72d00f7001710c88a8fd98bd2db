package parcial_diciembre_2017;

public class TarjetaDeEmpresa extends TarjetaPersonal implements ITarjetaVisita{
	private String job;
	public TarjetaDeEmpresa(String nombre, String correo, String puesto) {
		super(nombre, correo);
		job=puesto;
	}
	public String getNombre() {
		return name;
	}
	public String getEMail() {
		return email;
	}
	public String getJob() {
		return job;
	}

}
